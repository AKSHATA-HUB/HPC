#include <iostream>
#include <vector>
#include <stack>
using namespace std;

void DFS(int V, vector<vector<int>>& adj, int start) {
    vector<bool> visited(V, false);
    stack<int> s;
    s.push(start);

    cout << "DFS: ";
    while (!s.empty()) {
        int u = s.top(); s.pop();
        if (!visited[u]) {
            visited[u] = true;
            cout << u << " ";
            for (int v : adj[u]) {
                if (!visited[v]) {
                    s.push(v);
                }
            }
        }
    }
    cout << endl;
}

int main() {
    int V = 7;
    vector<vector<int>> adj(V);
    adj[0] = {1, 2};
    adj[1] = {0, 3, 4};
    adj[2] = {0, 5, 6};
    adj[3] = {1};
    adj[4] = {1};
    adj[5] = {2};
    adj[6] = {2};

    DFS(V, adj, 0);
 

    return 0;
}

